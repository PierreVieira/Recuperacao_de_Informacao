     "  {\n",
     "   \"cell_type\": \"code\",\n",
     "   \"execution_count\": null,\n",
     "   \"metadata\": {},\n",
     "   \"outputs\": [],\n",
     "   \"source\": [\n",
     "    \"def valores_iguais(lista1, lista2):\\n\",\n",
     "    \"    \\\"\\\"\\\"\\n\",\n",
     "    \"    @author: Pierre Vieira\\n\",\n",
     "    \"    :param lista1: lista de valores.\\n\",\n",
